.main {
  width: 100vw;
  background-color:#f4f4f5;
  display: flex;
  flex-wrap: wrap;
  align-content: flex-start;
  justify-content: center;
}
.header-bg {
  width: 100%;
  height: 100rpx;
  display: flex;
  background-color: #fff;
}

.logo-text {
  flex: 3;
  height: 92rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.logo-text-text{
  font-size: 35rpx;
  font-weight: bold;
  line-height: 36rpx;
  color: rgba(0, 0, 0, 1);
  text-align: left;
  vertical-align: top;
  height: 36rpx;
}
.logo {
  width: 41px;
  height: 100%;
}

.search {
  flex: 7;
  height: 46px;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

/* 以下是搜索框的样式 */
.searchbar {
  font-size: 14px;
  font-family: arial, sans-serif;
  color: #202124;
  display: flex;
  z-index: 3;
  height: 36px;
  background: white;
  border: 1px solid #dfe1e5;
  width: 264px;
  height: 32px;
  border-radius: 999px;  
  border: 2px solid rgba(59, 130, 246, 1);
  width: 90%;
}

.searchbar:hover {
  box-shadow: 0 1px 6px rgb(32 33 36 / 28%);
  border-color: rgba(223, 225, 229, 0);
}

.searchbar-wrapper {
  flex: 1;
  display: flex;
  height: 100%;
}


.search-icon-wrapper {
  margin: auto;
}
.searchbar-center {
  display: flex;
  flex: 7;
  flex-wrap: wrap;
  height: 100%;
}
.searchbar-right{
  display: flex;
  flex: 3;
  padding-left: 20rpx;
  margin: 0;
  justify-content: flex-end;
}
.searchbar-btn{
  border-radius: 999px;
  background: rgba(59, 130, 246, 1);
  padding: 4px 18px 4px 18px;
  color:#fff;
  font-size: 28rpx;
  font-weight: lighter;
  margin-top: 5rpx;
  margin-bottom: 5rpx;
}

.searchbar-input-spacer {
  color: transparent;
  flex: 100%;
  white-space: pre;
  height: 34px;
  font-size: 16px;
}

.searchbar-input {
  background-color: transparent;
  border: none;
  color: rgba(0, 0, 0, .87);
  word-wrap: break-word;
  outline: none;
  display: flex;
  flex: 100%;
  margin-top: -37px;
  height: 34px;
  font-size: 16px;
  max-width: 100%;
  width: 100%;
  font-size: 27rpx;
  font-weight: normal;
  letter-spacing: -0.24px;
  color:rgba(156, 163, 175, 1);
  line-height: 0px;
  text-align: left;
  vertical-align: top;
  padding-left: 10px;
  line-height: 34px;
}
.bar {
  width: 100%;
  height: 220rpx;
  background: rgba(255, 255, 255, 1);
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  
}
.bar-item-icon {
  width: 30px;
  height: 30px;
}
.bar-item {
  width: 25%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.bar-item-text {
  font-size: 30rpx;
  font-weight: normal;
  color:#000000 ;
  text-align: center;
  margin-top: 10rpx;
}
.alert-message{
  width: 90%;
  height: 74rpx;
  border: 1px solid rgba(191, 219, 254, 1);
  background: rgba(237, 245, 255, 1);
  border-radius: 6px;
  display: flex;
  flex-wrap: nowrap;
}
.alert-left{
  height: 100%;
  display:flex;
  justify-content: center;
  align-items: center;
  padding-left: 20px;
  flex:1;
}
.alert-right{
  height: 100%;
  display:flex;
  justify-content: center;
  align-items: center;
  flex:1;
}
.alert-cancel{
  width: 35rpx;
  height: 35rpx;

}
.alert-logo{
  width: 40rpx;
  height: 40rpx;

}
.alert-message-text{
  height:74rpx ;
  line-height: 74rpx;
  font-size: 30rpx;
  font-weight: normal;
  color: #3b82f6;
  margin-left: 10rpx;
  flex:8;
  
}
.alert-box{
  width: 100vw;
  display: flex;
  justify-content: center;
  background-color: #fff;
  padding-bottom: 30rpx;
}
.Recommended-food{
  margin-top: 20px;
  width: 97%;
  height: 346rpx;
  background-color: #fff;
  display: flex;
  border-radius: 8px;
  flex-wrap: wrap;
  margin-left: 3%;
}
.Recommended-food-text{
  width: 100%;
  height: 112rpx;
  line-height:112rpx;
  padding-left: 40rpx;
  letter-spacing: 3px;
  /** 文本1 */
font-size: 32rpx;
font-weight: bold;
display:flex;
flex-wrap: nowrap;
align-items: center;
background-color: #fff;
}
.Recommended-food-text-line{
  width: 10rpx;
  height: 30rpx;
  border-radius: 1px;
  background: rgba(59, 130, 246, 1);
  display: flex;
  margin-right: 10rpx;
}
.Recommended-food-pic{
  width: 100%;
  height: 232rpx;
  padding-bottom: 32rpx;
  padding-left: 50rpx;
  display: flex;
  flex-wrap: nowrap;
}
.Recommended-food-pic-item{
  width: 300rpx;
  height: 200rpx;
  border-radius: 12rpx;
  
}
.one-pic{
  width: 300rpx;
  height: 200rpx;
  margin-right: 16rpx;
  position: relative;
  overflow: hidden;
}
.Recommended-food-pic-title{
  font-size: 28rpx;
  color: rgb(255, 255, 255);
  height: 40rpx;
}
.Recommended-food-pic-text{
color: rgba(156, 163, 175, 1);
font-size: 24rpx;
line-height: 32rpx;
height: 32rpx;
width: 100%;
}
.Recommended-food-pic-bottom{
  position: absolute;
  top: 100rpx;
  padding-left: 16rpx;
  width: 100%;
  height: 100rpx;
  background: linear-gradient(180deg, rgba(0, 0, 0, 0.01) 0%, rgba(0, 0, 0, 0.8) 100%);
}
.tabbar{
  margin-top: 20rpx;
  width: 100vw;
  height: 88rpx;
  background-color: #fff;
}
.body {
  width: 100vw;
  margin-top: 0rpx;
  margin-bottom: 100px;
  padding-top: 30rpx;
}
.nav {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  font-size: 20px;
  font-weight: bold;
  color: rgba(0, 0, 0, 1);
}

.list {
  width: 100vw;
  justify-content: center;
  display: flex;
  flex-wrap: wrap;
  background-color: #fff;
}

.card {
  display: flex;
  flex-direction: row;
  width: 96%;
  margin-top: 15px;
}

.card-left {
  flex: 3
}

.card-right {
  flex: 7
}

.hangjz {
  display: flex;
  justify-content: space-between;
}

.card-title {
  margin-left: 8rpx;
  font-size: 20px;
  font-weight: normal;
  color: rgba(0, 0, 0, 1);
  overflow: hidden;
  height: 26px;
}
.card-distance {
  width: 50%;
  height: 26px;
  line-height: 26px;
  font-size: 14px;
  font-weight: 400;
  letter-spacing: 0.6px;
  color: #848b87;
  text-align: right;
}

.card-detail {
  margin-left: 8rpx;
  font-size: 14px;
  font-weight: 400;
  letter-spacing: -0.4px;
  width: 204px;
  height: 28px;
  line-height: 14px;
  opacity: 0.64;
  overflow: hidden;
  color: rgba(1, 15, 7, 1);
  margin-top: 5px;
}

.card-img {
  border-radius: 8rpx;
  width: 100px;
  height: 90px;

}

.card-star {
  font-size: 13px;
  font-weight: 400;
  letter-spacing: -0.5px;
  line-height: 0px;
  color: rgba(1, 15, 7, 1);
  text-align: left;
  margin-left: 8rpx;
  opacity: 0.74;
}
.card-price {
  margin-left: 10px;
  font-size: 13px;
  font-weight: 400;
  letter-spacing: -0.25px;
  line-height: 0px;
  color: rgba(1, 15, 7, 1);
  text-align: left;
}